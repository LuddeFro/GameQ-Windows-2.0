<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import java.net.URL?>
<VBox prefHeight="600.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.gameq.gameqwindows.ViewControllers.LoginView.LoginViewController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <TextField layoutX="99.0" layoutY="245.0" prefHeight="40.0" prefWidth="300.0" />
            <TextField layoutX="99.0" layoutY="308.0" prefHeight="40.0" prefWidth="300.0" />
            <Button fx:id="loginButton" layoutX="99.0" layoutY="386.0" mnemonicParsing="false" onAction="#processLogin" prefHeight="40.0" prefWidth="300.0" styleClass="button-orange" text="login " />
            <Button fx:id="signUp" layoutX="99.0" layoutY="444.0" mnemonicParsing="false" onAction="#gotoSignUp" prefHeight="40.0" prefWidth="300.0" styleClass="button-blue" text="sign up" />
      </children>
    </AnchorPane>
  </children>
  <stylesheets>
    <URL value="@LoginView.css" />
  </stylesheets>
</VBox>
